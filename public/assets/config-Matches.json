{
  "heading": "Match",
  "logo": "nrglogo_smaller.png",
  "pages": [
    {
      "name": "Setup",
      "widgets": [
        {
          "name": "Scouter Name",
          "type": "text"
        },
        {
          "name": "Starting Position",
          "type": "positions",
          "file": "field.png"
        }
      ]
    },
    {
      "name": "Autonomous",
      "widgets": [
        
        {
          "name": "Left Wing",
          "type": "checkbox",
          "col": 1
        },
        {
          "type": "spacer",
          "height": 20
        },
        {
          "name": "Speaker Notes",
          "type": "spinbox",
          "col": 1,
          "prefix": "Auto",
          "buttonColor": "darkorange"
        },
        {
          "name": "Amp Notes",
          "type": "spinbox",
          "col": 1,
          "prefix": "Auto",
          "buttonColor": "darkOrange"
        },
        {
          "name": "Picked Up From (All Ground)",
          "type": "heading",
          "colspan": 5,
          "align": "center"
        },
        {
          "name": "Wing",
          "type": "spinbox"
          },
        {
          "name": "Center",
          "type": "spinbox",
          "col" : 3,
          "row": 6
        }
      ]
    },
    {
      "name": "Teleop",
      "widgets": [
        {
          "name": "Picked Up From",
          "type": "heading",
          "colspan": 5,
          "align": "center"
        },
        {
          "name": "Ground",
          "type": "spinbox"
          },
        {
          "name": "Source",
          "type": "spinbox",
          "col" : 3,
          "row": 2
        },
        {
          "name": "Scoring",
          "type": "heading",
          "colspan": 5,
          "align": "center"
        },
        {
          "name": "Speaker Notes (Unamplified)",
          "type": "spinbox",
          "row": 5,
          "col": 1,
          "buttonColor": "orange"
        },
        {
          "name": "Speaker Notes (Amplified)",
          "type": "spinbox",
          "buttonColor": "orange"
        },
        {
          "name": "Amp Notes",
          "type": "spinbox",
          "buttonColor": "purple"
        },
        {
          "type": "spacer",
          "height": 20
        },
        {
          "name": "Defense Rating",
          "type": "dropdown",
          "options": [
            "0 (Not Played)",
            "1",
            "2",
            "3",
            "4",
            "5 (Very Effective)"
          ]
        },
        {
          "name": "Tipped",
          "type": "checkbox"
        }
      ]
    },
    {
      "name": "Endgame",
      "widgets": [
        {
          "name": "End Location",
          "type": "radio",
          "options": [
            "Onstage",
            "Parked",
            "None"
          ],
          "default": 2
        },
        {
          "name": "High Note Scored",
          "type": "checkbox"
        },
        {
          "name": "Trap Note",
          "type": "checkbox"
        },
        {
          "name": "Robot Sharing Chain",
          "type": "checkbox"
        },
        {
          "name": "Robots Onstage",
          "type": "spinbox",
          "max": 3
        },
        {
          "name": "Mechanical Failures",
          "type": "text"
        },
        {
          "name": "Comments",
          "type": "textarea"
        }
      ]
    }
  ]
}
